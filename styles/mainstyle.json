{
	"QMainWindow":[
		{
			"tittle":"PlannISEN",
			"icon":"icons/logo_login.png",
			"frameless": true,
			"transluscentBg": true,
			"navigation":[
				{
					"minimize":"minimizeBtn",
					"close": "closeBtn",
					"moveWindow": "headerContainer",
					"tittleBar": "headerContainer"
				}
			]
		}
	],
	"QCustomSlideMenu": [
		{
			"name": "popupContainer",
			"defaultSize": [{
				"width": 0,
				"height": 0
			}],
			"collapsedSize": [{
				"width": 0,
				"height": 0
			}],
			"expandedSize": [{
				"width": 700,
				"height": 500
			}],
			"floatPosition": [
				{
					"relativeTo": "mainBodyContent",
					"position": "center-center",
					"shadow":[
						{
							"color": "#000",
							"blurRadius": 30,
							"xOffset": 0,
							"yOffset": 0
						}
					]
				}
			],
			"menuTransitionAnimation": [{
				"animationDuration": 10,
				"animationEasingCurve": "Linear"
			}]
		}
	]
}
